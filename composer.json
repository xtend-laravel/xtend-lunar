{
    "name": "xtend-laravel/xtend-lunar",
    "authors": [
        {
            "name": "Adam Lee",
            "email": "adam@codelabx.ltd",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/xtend-laravel/xtend-lunar",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Xtend\\Extensions\\Lunar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xtend\\Extensions\\Lunar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xtend\\Extensions\\Lunar\\Providers\\AdminHubServiceProvider",
                "Xtend\\Extensions\\Lunar\\Providers\\LunarServiceProvider",
                "Xtend\\Extensions\\Lunar\\Providers\\LivewireTablesServiceProvider",
                "Xtend\\Extensions\\Lunar\\Providers\\ScoutDatabaseServiceProvider"
            ],
            "aliases": {
                "XtendLaravel": "Xtend\\Extensions\\Lunar\\Facades\\XtendLunar"
            }
        },
        "minimum-stability": "dev",
        "prefer-stable": true
    }
}
